{
  "ConnectionStrings": {
    "DefaultConnection": "Server=(localdb)\\mssqllocaldb;Database=weatherdb;Trusted_Connection=True;"
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "WeatherConfig": {
    "MinAmountOfDays": 1,
    "MaxAmountOfDays": 16,
    "DebugInfo": "false",
    "ExecutionTime": "10000"
  },
  "CitiesSet": {
    "Cities": [
      {
        "Name": "London",
        "Timer": 3600
      },
      {
        "Name": "Minsk",
        "Timer": 3600
      },
      {
        "Name": "Tbilisi",
        "Timer": 3600
      }
    ]
  }
}
